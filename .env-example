# GitHub Credentials
GITHUB_USERNAME=your_github_username
GITHUB_TOKEN=your_github_token

# Embedding Model Configuration
EMBEDDING_MODEL=jina-embeddings-v3
EMBEDDING_MODEL_API_KEY=your_jina_api_key
EMBEDDING_MODEL_URL=https://api.jina.ai/v1/embeddings

# Reranker Configuration
RERANKER_MODEL=jina-reranker-m0
RERANKER_URL=https://api.jina.ai/v1/rerank
RERANKER_TIMEOUT=30

# Chat LLM Configuration
CHAT_LLM_PROVIDER=openai  # Options: openai, ollama
CHAT_LLM_BASE_URL=https://openrouter.ai/api/v1
CHAT_LLM_MODEL=deepseek/deepseek-r1-0528:free
CHAT_LLM_API_KEY=your_openrouter_api_key

# Ollama Configuration
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=phi3.5:3.8b
OLLAMA_TIMEOUT=60

# Qdrant Configuration
QDRANT_URL=your_qdrant_url
QDRANT_API_KEY=your_qdrant_api_key
QDRANT_API_KEY_HEADER=api-key 